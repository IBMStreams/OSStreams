      <trans-unit xml:space="preserve" id="StreamsAuditMessages_CDISU4000I" resname="CDISU4000I" extraData="AuditSAMSubmitJob">
        <source xml:lang="en">The Streams Application Manager submitted application {0} of version {1} with submission parameters: {2}.</source>
        <!-- <MSGDOC>
          <REPLACEMENT arg="{0}" value="application_name"/>
          <REPLACEMENT arg="{1}" value="application_version"/>
          <REPLACEMENT arg="{2}" value="submission_parameters"/>
          <USER_RESPONSE>This message is for informational purposes only.</USER_RESPONSE>
        </MSGDOC> -->
        <target xml:lang="zh-TW">Streams Application Manager 已使用提交參數 {2}，來提交版本為 {1} 的應用程式 {0}。</target>
      </trans-unit>

      <trans-unit xml:space="preserve" id="StreamsAuditMessages_CDISU4001I" resname="CDISU4001I" extraData="AuditSAMCancelJob">
        <source xml:lang="en">The Streams Application Manager canceled job ID {0} with parameters: {1}.</source>
        <!-- <MSGDOC>
          <REPLACEMENT arg="{0}" value="job_id"/>
          <REPLACEMENT arg="{1}" value="cancel_parameters"/>
          <USER_RESPONSE>This message is for informational purposes only.</USER_RESPONSE>
        </MSGDOC> -->
        <target xml:lang="zh-TW">Streams Application Manager 已使用參數 {1} 來取消工作 ID {0}。</target>
      </trans-unit>

      <trans-unit xml:space="preserve" id="StreamsAuditMessages_CDISU4002I" resname="CDISU4002I" extraData="AuditSAMStreamPropertyEvent">
        <source xml:lang="en">The Streams Application Manager finished the {0} event for output port ID {1} with properties: {2}.</source>
        <!-- <MSGDOC>
          <REPLACEMENT arg="{0}" value="stream_property_event"/>
          <REPLACEMENT arg="{1}" value="output_port_id"/>
          <REPLACEMENT arg="{2}" value="properties"/>
          <USER_RESPONSE>This message is for informational purposes only.</USER_RESPONSE>
        </MSGDOC> -->
        <target xml:lang="zh-TW">Streams Application Manager 已針對具有下列內容的輸出埠 ID {1} 完成 {0} 事件：{2}。</target>
      </trans-unit>

      <trans-unit xml:space="preserve" id="StreamsAuditMessages_CDISU4003I" resname="CDISU4003I" extraData="AuditSAMOpenStreamView">
        <source xml:lang="en">The Streams Application Manager finished opening the {0} view for stream {1} with properties: {2}.</source>
        <!-- <MSGDOC>
          <REPLACEMENT arg="{0}" value="view_id"/>
          <REPLACEMENT arg="{1}" value="stream_id"/>
          <REPLACEMENT arg="{2}" value="properties"/>
          <USER_RESPONSE>This message is for informational purposes only.</USER_RESPONSE>
        </MSGDOC> -->
        <target xml:lang="zh-TW">Streams Application Manager 已針對具有下列內容的串流 {1} 完成開啟 {0} 視圖：{2}。</target>
      </trans-unit>

      <trans-unit xml:space="preserve" id="StreamsAuditMessages_CDISU4004E" resname="CDISU4004E" extraData="AuditSAMOperationFailureUserPermissionError">
        <source xml:lang="en">The Streams Application Manager did not complete the {0} operation due to the user {1} authorization failure. Please check log for more information.</source>
        <!-- <MSGDOC>
          <REPLACEMENT arg="{0}" value="operation"/>
          <REPLACEMENT arg="{1}" value="user"/>
          <EXPLANATION>The Streams Application Manager cannot complete the operation due to the user authorization failure.</EXPLANATION>
          <USER_RESPONSE>Update the user permission and try the operation again.</USER_RESPONSE>
        </MSGDOC> -->
        <target xml:lang="zh-TW">因為使用者 {1} 授權失效，Streams Application Manager 無法完成 {0} 作業。如需相關資訊，請檢查日誌。</target>
      </trans-unit>

      <trans-unit xml:space="preserve" id="StreamsAuditMessages_CDISU4005I" resname="CDISU4005I" extraData="AuditHCSetcapPE">
        <source xml:lang="en">IBM Streams set the {0} capabilities for the following PE: {1}. The file is: {2}.</source>
        <!-- <MSGDOC>
          <REPLACEMENT arg="{0}" value="capabilities"/>
          <REPLACEMENT arg="{1}" value="pe_id"/>
          <REPLACEMENT arg="{2}" value="file"/>
          <EXPLANATION>The IBM Streams host controller service set the capabilities for the PE.  This message is for informational purposes only.</EXPLANATION>
         <USER_RESPONSE>No action is required.</USER_RESPONSE>
        </MSGDOC> -->
        <target xml:lang="zh-TW">IBM Streams 已經為 PE {1} 設定 {0} 功能。檔案為：{2}。</target>
      </trans-unit>

      <trans-unit xml:space="preserve" id="StreamsAuditMessages_CDISU4006I" resname="CDISU4006I" extraData="AuditSAMOperationSuccessful">
