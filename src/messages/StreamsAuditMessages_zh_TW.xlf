      <trans-unit xml:space="preserve" id="StreamsAuditMessages_CDISU1000I" resname="CDISU1000I" extraData="AuditJMXOperationSuccessful">
        <source xml:lang="en">The operation was successful.</source>
        <!-- <MSGDOC>
          <USER_RESPONSE>This message is for informational purposes only.</USER_RESPONSE>
        </MSGDOC> -->
        <target xml:lang="zh-TW">作業已順利完成。</target>
